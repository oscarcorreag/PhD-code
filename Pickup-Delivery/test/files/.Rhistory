exp <- fread("populations_22Jul2019_152422.csv")
View(exp)
names(exp)[names(exp) == "V1"] <- "Density"
names(exp)[names(exp) == "V2"] <- "Normalized"
p1 <- ggplot(exp, aes(x = Density, y = Normalized, fill = Density))
p1 <- p1 + geom_boxplot()
p1 <- p1 + scale_x_discrete()
p1 <- p1 + my_theme()
p1 <- p1 + labs(y = "Intra-partition density")
